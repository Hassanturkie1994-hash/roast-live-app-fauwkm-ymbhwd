{
  "expo": {
    "name": "Roast Live",
    "slug": "Roast Live",
    "owner": "hasselite",
    "scheme": "roastlive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/f66cb126-9803-450a-8302-15c0e53a3af9.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/f66cb126-9803-450a-8302-15c0e53a3af9.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hasselite.roastlive",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Roast Live needs access to your camera to let you stream and use AR filters.",
        "NSMicrophoneUsageDescription": "Roast Live needs access to your microphone so others can hear you during the roast.",
        "NSPhotoLibraryUsageDescription": "Roast Live needs access to your photo library to share images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/f66cb126-9803-450a-8302-15c0e53a3af9.png",
        "backgroundColor": "#000000"
      },
      "package": "com.hasselite.roastlive",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/images/f66cb126-9803-450a-8302-15c0e53a3af9.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Roast Live needs access to your camera to let you stream and use AR filters.",
          "microphonePermission": "Roast Live needs access to your microphone so others can hear you during the roast.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  },
  "scheme": "Roast Live"
}